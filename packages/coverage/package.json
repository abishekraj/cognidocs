{
  "name": "@cognidocs/coverage",
  "version": "0.1.0",
  "description": "Coverage tracking for CogniDocs",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@cognidocs/types": "*",
    "@cognidocs/utils": "*"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "vitest": "^1.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "coverage",
    "documentation",
    "metrics"
  ],
  "license": "MIT"
}
